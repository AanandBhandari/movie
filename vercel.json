{
    "version": 2,
    "builds": [
      {
        "src": "./api/server.ts",
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/build",
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/server.ts"
      }
    ]
  }